    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "2.7.0",
    "eslint-config-zavatta": "^3.0.1",
    "eslint-config-zavatta-react": "^1.2.3",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
