{
  "name": "",
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "NODE_ENV=production babel-node bin/build",
    "preview": "npm run build && npm run prod",
    "prod": "NODE_ENV=production npm start",
    "start": "node bin/start",
    "lint": "eslint src webpack bin"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "module-resolver",
        {
          "root": [
            "src"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "zavatta",
      "zavatta-react"
    ]
  },
  "dependencies": {
    "compression": "^1.6.2",
    "es6-promisify": "^5.0.0",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-config-zavatta": "^4.2.0",
    "eslint-config-zavatta-react": "^2.1.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "2.0.0",
    "ignore-styles": "^5.0.1",
    "node-sass": "^4.5.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-stats-plugin": "^0.1.4"
